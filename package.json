{
  "name": "mysong",
  "version": "0.0.1",
  "description": "An app to share your favorite song.",
  "engines": {
    "node": "8.4.0"
  },
  "main": "client/index.html",
  "scripts": {
    "start": "cd server && node index.js",
    "heroku-postbuild": "cd client/ && npm install && npm run build",
    "start dev": "concurrently \"cd server && nodemon --watch ../db --watch . index.js \" \"cd client && npm start\"",
    "setup": "npm install && cd client && npm install",
    "build": "cd client && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TeamCRT/thesis.git"
  },
  "keywords": [
    "music",
    "songs",
    "song",
    "favorite",
    "share",
    "social",
    "listen",
    "listening",
    "spotify"
  ],
  "author": "TeamCRT",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TeamCRT/thesis/issues"
  },
  "homepage": "https://github.com/TeamCRT/thesis#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.1",
    "concurrently": "^3.5.1",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-session": "^1.15.6",
    "jquery": "^3.2.1",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "method-override": "^2.3.10",
    "mongodb": "^2.2.31",
    "mongoose": "^4.11.12",
    "nodemon": "^1.12.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-spotify": "^0.3.1",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1"
  }
}
