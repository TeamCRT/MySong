  },
  "homepage": "https://github.com/TeamCRT/thesis#readme",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.1",
    "concurrently": "^3.5.1",
    "connect-mongo": "^1.3.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-session": "^1.15.6",
    "mongodb": "^2.2.31",
    "mongoose": "^4.11.12",
